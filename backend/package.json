    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "nodemailer": "^6.9.5",
    "@aws-sdk/client-s3": "^3.400.0",
    "dayjs": "^1.11.10",
    "geoip-lite": "^1.4.7",
    "uuid": "^9.0.0"
