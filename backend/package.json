{
  "name": "lazysauce-backend",
  "version": "1.0.0",
  "description": "LazySauce Analytics Platform - Node.js Backend",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "test": "jest",
    "db:add-tenant-indexes": "node scripts/add-tenant-indexes.js",
    "db:add-tenant-indexes:dry": "node scripts/add-tenant-indexes.js --dry-run",
    "db:add-shared-indexes": "node scripts/add-shared-indexes.js",
    "db:add-shared-indexes:dry": "node scripts/add-shared-indexes.js --dry-run",
    "db:add-all-indexes": "node scripts/add-shared-indexes.js && node scripts/add-tenant-indexes.js",
